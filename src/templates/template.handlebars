
{{#each this}}
<div class="col-4 ">
   
   <div class="container-fluid px-0">
        <div class="row">
    <div class="col-md-12">
    <a class='' href="{{this.largeImageURL}}"> <img class="" src="{{this.webformatURL }}
" alt="" loading="lazy" max-width='400' max-height='200'  /></a>
  </div>
   </div>
      </div>


    <div class='d-flex flex-row mt-5'>
        <p class='d-flex flex-column' >
            <b>Likes</b>
            <span>{{this.likes}}</span>
        </p>
        <p  class='d-flex flex-column'>
            <b>Views</b>
            <span>{{this.views}}</span>

        </p>
        <p  class='d-flex flex-column'>
            <b>Comments</b>
            <span>{{this.comments}}</span>

        </p>
        <p  class='d-flex flex-column'>
            <b>Downloads</b>
           <span> {{this.downloads}}</span>
        </p>
    </div>
        </div>


{{/each}}